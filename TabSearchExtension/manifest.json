{
  "name": "Tab Searcher",
  "description": "Search your tabs by Title or Url instantly",
  "version": "1.0",
  "manifest_version": 3,
  "permissions":[
    "commands",
    "storage",
    "tabs"
  ],
  "action":{
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  },
  "options_page": "options.html"
}
